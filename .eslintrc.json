{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["react-app"],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react"],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
    }
  }